{
  "name": "tpl-ify",
  "version": "0.0.5",
  "description": "Simple template engine",
  "main": "build/tplify.js",
  "scripts": {
    "start": "npm run beautify && npm run min",
    "beautify": "uglifyjs src/tplify.js -b -o build/tplify.js",
