{
  "name": "tplify",
  "version": "0.0.1",
  "description": "A simple template engine.",
  "main": "build/tplify.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm run beautify && npm run min",
    "beautify": "uglifyjs src/tplify.js -b -o build/tplify.js",
    "min": "uglifyjs src/tplify.js -m -o build/tplify.min.js",
    "test": "node test/node.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noify/tplify.git"
  },
  "keywords": [
    "template",
    "engine",
    "js"
  ],
  "author": "noify",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/noify/tplify/issues"
  },
  "homepage": "https://github.com/noify/tplify#readme",
  "devDependencies": {
    "uglify-js": "^3.0.28"
  }
}
