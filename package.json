    "test": "mocha test/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noify/tplify.git"
  },
  "keywords": [
    "template",
    "engine",
    "js"
  ],
  "author": "noify",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/noify/tplify/issues"
  },
  "homepage": "https://github.com/noify/tplify#readme",
  "devDependencies": {
    "uglify-js": "^3.1.8",
    "mocha": "^4.0.1",
    "chai": "^4.1.2"
  }
}
